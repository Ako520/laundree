<!DOCTYPE html>
<html>
<head>
    {{> head}}
</head>
<body class="app">
{{> media}}

{{#unless no_nav}}
    {{> topnav}}
{{/unless}}

<div id="MenuExpander">
    <svg>
        <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#MenuLines"></use>
    </svg>
    <svg class="close">
        <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#CloseX"></use>
    </svg>
</div>
<nav id="LeftNav">
    <ul>
        <li data-label="Timetable">
            <a href="/">
                <svg>
                    <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#Time"></use>
                </svg>
                <span>
                Timetable
                </span>
            </a>
        </li>
        <li data-label="Your bookings">
            <a href="/">
                <svg>
                    <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#List"></use>
                </svg>
                <span>
                Your bookings
                </span>
            </a>
        </li>
    </ul>
    <hr/>
    <ul>
        <li data-label="Settings">
            <a href="/">
                <svg>
                    <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#Gears"></use>
                </svg>
                <span>
                Settings
                </span>
            </a>
        </li>

    </ul>
</nav>
<div id="RightContent">
    {{{ body }}}
</div>
</body>
<script type="application/javascript" src="/javascripts/bundle.js"></script>
</html>
