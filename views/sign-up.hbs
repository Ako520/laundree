<main id="SignUpPage">
    <a href="/">
        <svg id="Logo">
            <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#Logo"></use>
        </svg>
    </a>
    <ul id="StepList" class="">
        <li>
            <svg class="step step1">
                <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#Step1"></use>
            </svg>
        </li>
        <li class="dots dots1">
            <svg>
                <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#Dots"></use>
            </svg>
        </li>
        <li>
            <svg class="step step2">
                <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#Step2"></use>
            </svg>
        </li>
        <li class="dots dots2">
            <svg>
                <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#Dots"></use>
            </svg>
        </li>
        <li>
            <svg class="step step3">
                <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#Step3"></use>
            </svg>
        </li>
    </ul>
    <section id="CreateAccount">
        <h1>
            Create an account
        </h1>
        <form class="validate initial">
            <label data-validate-error="Please enter your full name" class="validate">
                <input type="text" placeholder="Full name" data-validate-type="non-empty"/>
            </label>
            <label data-validate-error="Please enter a valid e-mail address" class="validate">
                <input type="text" placeholder="E-mail address" data-validate-type="email"/>
            </label>
            <label data-validate-error="Min. 6 characters containing at least one letter" class="validate">
                <input type="password" placeholder="Password" data-validate-type="password" />
            </label>
            <label class="validate" data-validate-error="Please accept our conditions">
                <input type="checkbox" data-validate-type="checked"/>
                Accept terms and conditions
            </label>
            <div class="buttons">
                <button class="prev inactive">Back</button>
                <button class="next continue">Next</button>
            </div>
        </form>
    </section>
    <section id="RegisterMachines" hidden>
        <h1>
            Register your machines
        </h1>
        <form class="initial validate">
            <ul>
                <li>
                    <div class="select_machine select">
                        <span class="value">
                            <svg class="machine">
                                <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#WashingMachine"></use>
                            </svg>
                            <svg class="chevron">
                                <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#ChevronDown"></use>
                            </svg>

                        </span>
                        <input type="hidden" name="type"/>
                        <ul class="options">
                            <li>
                                <svg class="machine">
                                    <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#WashingMachine"></use>
                                </svg>
                                <span class="desc">
                                    Washing machine
                                </span>
                            </li>
                            <li>
                                <svg class="machine">
                                    <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#Dryer"></use>
                                </svg>
                                <span class="desc">
                                    Tumble dryer
                                </span>
                            </li>
                        </ul>
                    </div>
                    <label class="validate" data-validate-error="Please enter a unique machine-name">
                        <input type="text" placeholder="Machine name" name="machine_name" data-validate-type="non-empty unique"/>
                    </label>
                    <svg class="delete">
                        <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#Trash"></use>
                    </svg>
                </li>
            </ul>
            <div class="add">
                <svg>
                    <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#Plus"></use>
                </svg>
                Add machine
            </div>
            <div class="buttons">
                <button class="prev">Back</button>
                <button class="next continue">Next</button>
            </div>

        </form>
    </section>
