{{#> email_layout.hbs title=subject}}
    {{ formatIntl
            'email.contact-receipt.message'
            messages=messages
            name=name
            nl='<br />'
            message=(format '<pre>{message}</pre>' message=message)}}
    <br/><br/>
    {{ formatIntl
            'email.signature'
            messages=messages
            nl='<br />' }}
{{/email_layout.hbs}}
